        {
          "title": "Error-Response:",
          "content": "HTTP/1.1 500 No Response\n{\n  \"error\": \"error in sending message\"\n}",
          "type": "json"
        }
      ]
    },
    "version": "0.0.0",
    "filename": "bin/api_doc.js",
    "groupTitle": "SEND_SMS"
